{
  "manifest_version": 3,
  "name": "Keyboard Sounds",
  "version": "1.0",
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["sounds/click.mp3"],
      "matches": ["<all_urls>"]
    }
  ]
}
